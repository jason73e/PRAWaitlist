                        <dd>
                            @Html.DisplayFor(model => parent.UpdateUserID)
                        </dd>
                    </dl>
                </div>
                <div class="form-horizontal  col-md-2">
                    <div class="form-group">
                        <div class="col-md-offset-2 col-md-10">
                            @Html.ActionLink("Parent Notified", "Notified", new { lmId = Model.lm.Id, sParentName = parent.FirstName + " " + parent.LastName }, new { @class = "btn btn-primary" })
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-md-offset-2 col-md-10">
                            @Html.ActionLink("Parent Accepted", "Accepted", new { lmId = Model.lm.Id, sParentName = parent.FirstName + " " + parent.LastName }, new { @class = "btn btn-primary" })
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-md-offset-2 col-md-10">
                            @Html.ActionLink("Parent Declined", "Declined", new { lmId = Model.lm.Id, sParentName = parent.FirstName + " " + parent.LastName }, new { @class = "btn btn-primary" })
                        </div>
                    </div>
                </div>
