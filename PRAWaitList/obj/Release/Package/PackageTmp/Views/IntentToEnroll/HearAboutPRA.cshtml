@model PRAWaitList.Models.IntentToEnrollViewModel
<div class="checkbox">
    @foreach (var LearnPRA in Model.lsLearnPRAList)
    {
        var sChecked = "";
        if (Model.sm.LearnAboutPRA != null && Model.sm.LearnAboutPRA.Contains(LearnPRA))
        {
            sChecked = "checked";
            Model.sm.LearnAboutPRA=Model.sm.LearnAboutPRA.Replace(LearnPRA + ",", string.Empty);
        }
        <label>
            <input type="checkbox" @sChecked name="Student.sm.LearnAboutPRA" id="Student_sm_LearnAboutPRA" value="@LearnPRA">@LearnPRA
        </label>
    }
</div>